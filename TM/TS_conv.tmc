%{

  #define to_double(x) (double)x
%}
AmbTS_T_t TS0_T; Invalidate TS0_T;
{
  if (TS0_cnt > 0) {
    TS0_T = (to_double(TS0_raw))/TS0_cnt;
    TS0_T = 1.25 * TS0_T/8388608.;
  } else {
    TS0_T = -10.0;
  }
  Validate TS0_T;
}

